{
  "all": true,
  "check-coverage": true,
  "branches": 75,
  "lines": 75,
  "functions": 75,
  "statements": 75,
  "watermarks": {
    "lines": [70, 95],
    "functions": [70, 95],
    "branches": [70, 95],
    "statements": [70, 95]
  },
  "reporter": [
    "text",
    "html"
  ],
  "report-dir": "./dist/test-coverage",
  "skip-full": true,
  "include": [
    "database/**/*.js",
    "src/**/*.js",
    "utils/**/*.js"
  ],
  "exclude": [
    "**/*.spec.js",
    "utils/query/getModelByName.js"
  ],
  "cache": false
}
